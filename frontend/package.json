{
  "name": "nextly-template",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "version": "0.1.0",
  "description": "Nextly Landing Page Template",
  "private": true,
  "scripts": {
    "dev": "npm run restore-full-font && next dev --webpack",
    "restore-full-font": "cp public/fonts/ChenYuluoyan-2.0-Thin.full.woff2 public/fonts/ChenYuluoyan-2.0-Thin.woff2",
    "build": "npm run restore-full-font && NEXT_OUTPUT=export next build && node scripts/subset-font.js",
    "deploy": "npm run build && firebase deploy",
    "start": "next start",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:all": "node scripts/lighthouse-audit.js --all",
    "lighthouse:compact": "node scripts/compact-lighthouse.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gituhb.com/web3templates/nextly-template"
  },
  "keywords": [
    "nextjs",
    "tailwindcss",
    "free"
  ],
  "author": "Surjith S M",
  "license": "ISC",
  "dependencies": {
    "@dotlottie/react-player": "^1.6.19",
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@iconify/react": "^6.0.2",
    "@line/liff": "^2.27.3",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "firebase-admin": "^13.6.0",
    "framer-motion": "^12.23.26",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "next": "^16.1.1",
    "next-themes": "^0.3.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-scroll-parallax": "^3.5.0",
    "remark-gfm": "^4.0.1",
    "shadergradient": "^1.3.5",
    "survey-core": "^2.5.5",
    "survey-react-ui": "^2.5.5",
    "swiper": "^12.0.3",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "cheerio": "^1.1.2",
    "daisyui": "^5.5.14",
    "fontmin": "^1.1.1",
    "glob": "^13.0.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}